    "list_numbers = []\n",
    "n_th_power = 5\n",
    "max_number = n_th_power*(9**n_th_power)\n",
    "\n",
    "resulst = 0\n",
    "\n",
    "for number in range(10,max_number+1):\n",
    "    if sum([int(digit)**(5) for digit in str(number)]) == number:\n",
    "        resulst = resulst + number\n",
    "\n",
    "resulst\n"
   ]
