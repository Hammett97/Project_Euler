   "source": [
    "def Multiples3or5(n):\n",
    "    if (n % 3 == 0) or (n % 5 == 0):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "    \n",
    "sum = 0\n",
    "for i in range(1000):\n",
    "    if Multiples3or5(i):\n",
    "        sum = sum + i\n",
    "\n",
    "print(sum)"
   ]
  }
