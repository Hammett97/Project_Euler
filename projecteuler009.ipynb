   "source": [
    "for a in range(1,1000):\n",
    "    for b in range(1, 1000):\n",
    "        if ( (a**2 + b**2) == (1000 - a - b)**2 ):\n",
    "            result = (a * b * (1000 - a - b))\n",
    "            break\n",
    "\n",
    "print(result)\n"
   ]
  }
